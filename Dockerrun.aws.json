{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
    "name": "django-polls",
    "image": "anreddy/django-polls",
    "hostName": "django-polls",
    "memory": 128,
    "essential": true,
    "portMappings": [{
      "hostPort": 80,
      "containerPort": 8080
    },{
      "hostPort": 443,
      "containerPort": 8080
    }]
  }]
}


